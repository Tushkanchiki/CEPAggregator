@model Dictionary<string, string>

<div class="container">
    <div class="row">
        <div class="col-sm pt-3">
            Тут будет карта
        </div>

        <div class="col-sm pt-3" style="font-size: x-large">
            @foreach (var pair in Model)
            {
                <p class="mb-0">
                    <span style="font-weight: bold">
                        @string.Concat(pair.Key, ": ")
                    </span>
                    <span>
                        @pair.Value
                    </span>
                </p>
            }
        </div>
    </div>
</div>
